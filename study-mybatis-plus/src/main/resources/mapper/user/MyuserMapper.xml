<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.christli.study.mybatis.plus.user.dao.MyuserMapper">

    <!-- 举一个sql案例 -->
    <select id="getUserByName"
